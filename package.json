{
  "name": "leaflet.sentimentheat",
  "version": "0.1.0",
  "description": "A Leaflet heatmap plugin.",
  "homepage": "https://github.com/adrianlyjak/Leaflet.heat",
  "keywords": [
    "heatmap",
    "canvas",
    "visualization",
    "gis",
    "leaflet",
    "plugin"
  ],
  "author": "Adrian Lyjak",
  "repository": {
    "type": "git",
    "url": "git://github.com/Leaflet/Leaflet.heat.git"
  },
  "main": "dist/leaflet-heat.js",
  "devDependencies": {
    "eslint": "^1.7.3",
    "eslint-config-mourner": "^1.0.1",
    "uglify-js": "^2.5.0"
  },
  "eslintConfig": {
    "extends": "mourner",
    "globals": {
      "L": false,
      "simpleheat": false
    }
  },
  "scripts": {
    "test": "eslint src",
    "prepublish": "uglifyjs src/simpleheat.js src/HeatLayer.js -c -m --source-map dist/leaflet-heat.js.map --source-map-include-sources -o dist/leaflet-heat.js"
  },
  "license": "BSD-2-Clause",
  "jshintConfig": {
    "quotmark": "single",
    "globals": {
      "L": true,
      "simpleheat": true
    },
    "trailing": true,
    "camelcase": true,
    "curly": true,
    "eqeqeq": true,
    "noempty": true,
    "nonbsp": true,
    "undef": true,
    "unused": true,
    "browser": true
  }
}
